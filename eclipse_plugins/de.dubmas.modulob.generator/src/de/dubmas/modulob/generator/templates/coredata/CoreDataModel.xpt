#pragma mark singleton methods
//for singleton implementation see:
//http://stackoverflow.com/questions/145154/what-does-your-objective-c-singleton-look-like

+ (CoreDataDAO*)sharedDAO
{
	@synchronized(self) {
		if («this.daoName().toFirstLower()» == nil) {
			[[self alloc] init]; // assignment not done here
		}
	}
	return «this.daoName().toFirstLower()»;
}

+ (id)allocWithZone:(NSZone *)zone
{
	@synchronized(self) {
		if («this.daoName().toFirstLower()» == nil) {
			«this.daoName().toFirstLower()» = [super allocWithZone:zone];
			return «this.daoName().toFirstLower()»;  // assignment and return on first allocation
		}
	}
	return nil; //on subsequent allocation attempts return nil
}

- (id)copyWithZone:(NSZone *)zone
{
	return self;
}

- (id)retain
{
	return self;
}

- (unsigned)retainCount
{
	return UINT_MAX;  //denotes an object that cannot be released
} 

