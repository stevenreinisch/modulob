<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="de.dubmas.modulob.migration.popupMenu"
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="false"
            id="de.dubmas.modulob.migration.ui.objectContribution1"
            nameFilter="*"
            objectClass="org.eclipse.core.resources.IFile">
         <action
               class="de.dubmas.modulob.migration.ui.actions.StartMigrationAction"
               enablesFor="1"
               id="de.dubmas.modulob.migration.ui.action1"
               label="Specify Migration"
               menubarPath="additions">
         </action>
      </objectContribution>
      
      <objectContribution
            adaptable="false"
            id="de.dubmas.modulob.migration.ui.objectContribution2"
            nameFilter="*"
            objectClass="org.eclipse.core.resources.IFile">
         <action
               class="de.dubmas.modulob.migration.ui.actions.CreateNewEntityModelFileAction"
               enablesFor="1"
               id="de.dubmas.modulob.migration.ui.action1"
               label="New Version"
               menubarPath="additions"
               tooltip="Create a new version of an entity model.">
         </action>
      </objectContribution>
   </extension>
</plugin>
